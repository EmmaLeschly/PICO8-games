--menu-functions
function point()
 cell = {
  x=rnd(128),
  y=rnd(128),
  col = rnd(2)+5,
  update = function(self)
    
  
   self.x = self.x-(self.col*2)/40
   if self.x <0 then 
    self.x = 127 
    self.y = rnd(88)+12
   end  
  end,
  draw = function(self)
   pset(self.x,self.y,self.col)
  end,
 }
 return cell

end

cell_list = {

}


function background(cell,updateOrdraw)
 if updateOrdraw == update then
  for cell in all(cell_list) do
   cell:update()
  end
 end

 if updateOrdraw == draw then
  for cell in all(cell_list) do
   cell:draw()
  end
 end
end

