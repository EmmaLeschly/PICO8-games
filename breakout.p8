--breakout
--by emma

function _init()
	state="menu"

	if state=="menu" then
		cls()	
		startgame()
 	makepad()
 	makeball()
 	makebrick()
  starinit()
 end
end

function _update()
	if state=="menu" then
		update_menu()
	elseif state=="game" then
		update_game()
	elseif state=="game over" then
		update_gameover()
	end
end

function _draw()
	if state=="menu" then
		draw_menu()
	elseif state=="game" then
		draw_game()
	elseif state=="game over" then
		draw_gameover()
	end
end

function update_menu()
	if btnp(❎) then
		state="game"
	end
end

function update_game()
	if btnp(❎) then
		state="menu"
	end
	
	movepad()
	moveball()
	collision()
	collisionbrick()
	losedeadball()
end

function draw_menu()
	cls()
	rectfill(0,0,128,128,0)
	for i=0,5,1 do
		print("✽✽super breakout✽✽",21,55-i,12-i)
	end

	print("press ❎ to start game",23,70,7)
end

function draw_game()
	cls()
	drawstars()
	drawpad()
	drawball()
	drawbrick()
	heartsandscore()
end

function update_gameover()
	if btnp(❎) then
		state="menu"
		lives=3
		score=0
	end
end

function draw_gameover()
	rectfill(0,0,128,128,0)
	print("◆◆game over◆◆",35,60,12)
	print("◆◆game over◆◆",35,59,11)
	print("◆◆game over◆◆",35,58,10)
	print("◆◆game over◆◆",35,57,9)
	print("◆◆game over◆◆",35,56,8)
	print("◆◆game over◆◆",35,55,7)
	print("press ❎ to play again",23,70,7)
	print("score:",45,80,7) 
	print(score,70,80,7)
end
