--paddle
	
function makepad()
	pad={x=64,y=120,dx=0,w=24,h=3,c=7}

end

function movepad()
--right/left
	bpress=false
	if btn(0) then
		pad.dx-=2
		bpress=true
	end
	if btn(1) then 
		pad.dx+=2
		bpress=true
	end
	
	pad.dx=pad.dx/1.2
	pad.x+=pad.dx
	
--stop
	if pad.x<0 then
		pad.x=0
	end
	if pad.x>128-pad.w then
		pad.x = 128-pad.w
	end
end

function drawpad()
	rectfill(pad.x,pad.y,pad.x+pad.w,pad.y+pad.h,pad.c)
end

