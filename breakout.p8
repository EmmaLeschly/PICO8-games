--ball

function makeball()
	ball={x=64,y=110,xspeed=2,yspeed=2,r=2,dr=0.5,w=4,h=4}
	
	rayx=0
	rayy=0
	raydx=-2
	raydy=-2
end

function moveball()
	ball.nextx=ball.x+ball.xspeed
	ball.nexty=ball.y+ball.yspeed
	
	ball.x=ball.nextx
	ball.y=ball.nexty
	
	ball.r=ball.r+ball.dr
	
	if ball.r > 3 or ball.r < 2 then
		ball.dr=-ball.dr
	end

	if ball.y<0 then
		ball.yspeed=ball.yspeed*-1
		sfx(0)
	end
	if ball.x<0 or ball.x>128-ball.w then
		ball.xspeed=ball.xspeed*-1
		sfx(0)
	end
	
	pad.c=7
	
	if collision() then
		pad.c=14
		sfx(0)
		ball.yspeed=ball.yspeed*-1
		score+=10
	end
	
end

function collision()
--check for collision w. pad
		if ball.y-ball.r>pad.y then return false end
		if ball.y+ball.r<pad.y then return false end 
		if ball.x-ball.r>pad.x+pad.w then return false end
		if ball.x+ball.r<pad.x then return false end 
return true
end

function drawball()
	circfill(ball.x,ball.y,ball.r,7)
end

function losedeadball()
	if ball.y>128 then
			--next life
			sfx(1)
			ball.y=30
			lives-=1
	end
end

function collisionbrick()
	for b in all(bricks) do
		if between(ball.nextx, ball.w,b.x,brick.w) and
			between(ball.nexty,ball.h,b.y,brick.h) then
		del(bricks,b)
		score+=10
		sfx(0)
		if between(ball.x,ball.w,b.x,brick.w) then
			ball.yspeed=-ball.yspeed
		elseif between(ball.y,ball.h,b.y.brick.h) then
			ball.xspeed=-ball.xspeed
		else
			ball.xspeed=-ball.xspeed
			ball.yspeed=-ball.yspeed
		end
	end
end
end

function between(x1,w1,x2,w2)
	if x1<x2+w2 then
		if x1+w1>x2 then
			return true
		end
	end
	return false
end
