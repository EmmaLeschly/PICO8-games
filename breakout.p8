--titlescreens og animation

function startgame()
	mode="game"
	score=0
	lives=3
end

--animation
function starinit()
	timer = {}
	timer.t = 0
	timer.c = 0
	
	star = {}
	star.start = 0
	star.num = 120

	star_array = {}
	gen_stars()
end

function gen_stars()
	for row = 1, star.num do
	    star_array[row] = {}
	
	    for column = 1, star.num do
		    star.rand = rnd(star.num)
						star.rand = flr(star.rand)
			
	     star_array[row][column] = star.rand
	   end
	end
end
		
function star_ani()
	local x_move = {}
	local color_flash = 7
	for y = 1, 10 do
	    for x = 1, 10 do
		  x_move = (star_array[y][x]) + timer.t    
		  local y_pos = {}		  
		  	
			if  (x_move >0) then
				 y_pos = star_array[x][y] + (timer.t / 10)
				x_move = x_move * (x_move / 100)
				
				if  (x_move < 130) then
					color_flash = 7
				end
				if  (x_move > 130) then
					x_move =  200 - (x_move / 2)
					
					color_flash = 5
				end
			end
			
			if  (x_move <0) then
				y_pos = star_array[x][y] - (timer.t / 10)
				x_move = x_move * (x_move / 100)
				if  (x_move < 130) then
					color_flash = 5
				end
				if  (x_move > -200) then
					x_move =  1 + (x_move * 2)
					color_flash = 7
				end
			end

			pset(x_move, y_pos,  color_flash)
	    end
	end
	if timer.t <= 250 then
		timer.t = timer.t + 1
		end
	if timer.t >= 250 then
		timer.t =  -200
	end
end

function drawstars()
	cls()
	star_ani()
end
		
