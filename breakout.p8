--levels,lives,score

function heartsandscore()
	for i=1,lives do
		spr(0,90+i*8,4)
	end
	
	--draw score
	rectfill(8,4,28,12,7)
	print(score,10,6,0)
	
	if lives==0 then
		state="game over"
	end
	
	if count(bricks)==0 then
		
		makebrick()
	end
end



